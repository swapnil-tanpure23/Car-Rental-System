<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>Home.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>AddGuest</servlet-name>
        <servlet-class>RentalSystem.AddGuest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddUser</servlet-name>
        <servlet-class>RentalSystem.AddUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>RentalSystem.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteGuest</servlet-name>
        <servlet-class>RentalSystem.DeleteGuest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PickUp</servlet-name>
        <servlet-class>RentalSystem.PickUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Return</servlet-name>
        <servlet-class>RentalSystem.Return</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LookUp</servlet-name>
        <servlet-class>RentalSystem.LookUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reservation</servlet-name>
        <servlet-class>RentalSystem.Reservation</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddGuest</servlet-name>
        <url-pattern>/AddGuest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddUser</servlet-name>
        <url-pattern>/AddUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteGuest</servlet-name>
        <url-pattern>/DeleteGuest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PickUp</servlet-name>
        <url-pattern>/PickUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Return</servlet-name>
        <url-pattern>/Return</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LookUp</servlet-name>
        <url-pattern>/LookUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reservation</servlet-name>
        <url-pattern>/Reservation</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
